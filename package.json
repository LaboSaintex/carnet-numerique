{
  "name": "dodoc2",
  "productName": "do•doc",
  "version": "7.0.0-dev",
  "description": "do•doc is a documentation tool for creativity.",
  "author": "L’Atelier des Chercheurs <https://latelier-des-chercheurs.fr/>",
  "homepage": "https://github.com/l-atelier-des-chercheurs/dodoc#readme",
  "license": "CC-BY-NC-SA-4.0",
  "main": "main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc.git"
  },
  "bugs": {
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc/issues"
  },
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=5858 . --debug --verbose",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "electronVersion": "1.8.2",
    "appId": "dodoc2",
    "productName": "do•doc",
    "asar": false,
    "mac": {
      "category": "public.app-category.education"
    }
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "body-parser": "^1.18.2",
    "dodoc-parsedown": "git://github.com/louis-ev/dodoc-parsedown.git#master",
    "electron-context-menu": "^0.9.1",
    "electron-devtools-installer": "^2.2.3",
    "electron-log": "^2.2.14",
    "exif-reader": "^1.0.2",
    "express": "^4.16.3",
    "ffmpeg-static": "^2.2.0",
    "ffprobe-static": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^1.2.0",
    "fs-extra": "^5.0.0",
    "gulp-util": "^3.0.8",
    "moment": "^2.21.0",
    "node-localstorage": "^1.3.1",
    "parser-readline": "^1.0.2",
    "path": "^0.12.7",
    "portscanner": "^2.1.1",
    "pug": "^2.0.2",
    "sharp": "^0.19.1",
    "slugg": "^1.2.1",
    "socket.io": "^2.0.4",
    "validator": "^9.4.1",
    "vue": "^2.5.16",
    "vue-i18n": "^7.6.0",
    "write-file-atomic": "^2.3.0"
  },
  "devDependencies": {
    "electron": "1.8.2",
    "electron-builder": "^19.56.2",
    "electron-packager": "^11.0.1",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.7.3",
    "node-gyp": "^3.6.2"
  }
}
