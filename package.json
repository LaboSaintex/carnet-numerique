{
  "name": "dodoc",
  "productName": "do•doc",
  "version": "9.0.0-dev",
  "description": "do•doc is a documentation tool for creativity.",
  "author": "L’Atelier des Chercheurs <https://latelier-des-chercheurs.fr/>",
  "homepage": "https://github.com/l-atelier-des-chercheurs/dodoc#readme",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc.git"
  },
  "bugs": {
    "url": "https://github.com/l-atelier-des-chercheurs/dodoc/issues"
  },
  "scripts": {
    "start": "electron-forge start",
    "debug": "electron --inspect=5858 . --debug --verbose",
    "debug-brk": "electron --inspect=5858 . --debug-brk --verbose",
    "node-start": "node .",
    "node-debug": "node . --inspect=5858 --debug --verbose",
    "node-debug-brk": "node . --inspect=5858 --debug-brk --verbose",
    "pack": "build --dir",
    "dist": "build",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "engines": {
    "node": "8.9.3",
    "npm": "*"
  },
  "build": {
    "electronVersion": "2.0.16",
    "appId": "dodoc2",
    "productName": "do•doc",
    "asar": false,
    "mac": {
      "category": "public.app-category.education"
    }
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dodoc-parsedown": "github:louis-ev/dodoc-parsedown#master",
    "electron-compile": "^6.4.4",
    "electron-pdf-window": "^1.0.12",
    "electron-window-state": "^5.0.3",
    "exif-reader": "^1.0.3",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "formidable": "^1.2.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "gulp-util": "^3.0.8",
    "memory-cache": "^0.2.0",
    "moment": "^2.24.0",
    "node-localstorage": "^2.1.5",
    "pad-left": "^2.1.0",
    "parser-readline": "^1.0.2",
    "path": "^0.12.7",
    "platform-folders": "^0.4.1",
    "portscanner": "^2.2.0",
    "pug": "^2.0.4",
    "quill-render": "^1.0.5",
    "rich-text": "^4.0.0",
    "sharedb": "^1.1.0",
    "sharedb-logger": "^0.1.4",
    "sharp": "0.17.3",
    "slugg": "^1.2.1",
    "socket.io": "^2.3.0",
    "source-map": "^0.7.3",
    "spark-md5": "^3.0.0",
    "uuid": "^3.3.3",
    "validator": "^12.1.0",
    "websocket-json-stream": "0.0.3",
    "write-file-atomic": "^3.0.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "2.0.16",
    "electron-rebuild": "^1.8.2",
    "node-gyp": "^3.8.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "executableName": "dodoc",
        "icon": "build/icon.ico",
        "appCategoryType": "public.app-category.education"
      },
      "electronWinstallerConfig": {
        "name": "dodoc"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": "",
        "draft": true
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "do•doc"
      }
    }
  }
}
